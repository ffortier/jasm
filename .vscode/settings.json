{
    "files.associations": {
        "stdio.h": "c"
    },
    "[python]": {
        "editor.defaultFormatter": "ms-python.autopep8"
    },
    "python.formatting.provider": "none",
    "java.project.referencedLibraries": [
        "bazel-bin/external/maven/**/*.jar",
        "bazel-bin/instruction/**/*.jar"
    ],
    "java.project.sourcePaths": [
        "**/src/main/java",
        "**/src/test/java",
    ],
}
